#include "nhits.h"

nhits::nhits():Tool(){}


bool nhits::Initialise(std::string configfile, DataModel &data){

  if(configfile!="")  m_variables.Initialise(configfile);
  //m_variables.Print();

  m_data= &data;

  return true;
}


bool nhits::Execute(){

  return true;
}


bool nhits::Finalise(){

  return true;
}
